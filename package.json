{
  "name": "microservices-boilerplate",
  "version": "1.0.0",
  "scripts": {
    "lint": "yarn eslint .",
    "snyk-setup": "yarn snyk auth && yarn snyk test",
    "lint-fix": "yarn eslint . --fix",
    "start": "yarn ts-node-dev --respawn --watch ./.env,./.env.*,./src/swagger,./config ./src/index.ts",
    "build": "yarn tsup && yarn copy-files",
    "copy-files": "copyfiles -u 1 \"src/swagger/swagger.yaml\" \"dist\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test-repo": "yarn jest src/tests/repo.test.ts",
    "generate-openapi-schemas": "yarn typeconv -f ts -t oapi -o src/swagger/schemas src/types/*.ts",
    "generate-openapi-spec": "yarn ts-node src/swagger/openapi-spec-generator.ts",
    "generate-db-url": "yarn ts-node src/utils/utils.repos.ts",
    "migrate-db": "yarn prisma migrate dev --schema ./src/prisma/schema.prisma && yarn prisma generate --schema ./src/prisma/schema.prisma",
    "load-seed-data": "yarn ts-node src/repos/seed.ts",
    "queue-admin-setup": "cd queue-admin && yarn install",
    "init-db": "yarn generate-db-url && yarn migrate-db && yarn load-seed-data",
    "setup": "yarn generate-openapi-schemas && yarn generate-openapi-spec && yarn init-db",
    "prepare": "husky install",
    "queue-admin": "cd queue-admin && yarn run dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.649.0",
    "@bull-board/api": "^5.21.4",
    "@bull-board/express": "^5.21.4",
    "@bull-board/ui": "5.21.4",
    "@prisma/client": "^5.19.0",
    "@types/body-parser-xml": "^2.0.5",
    "@types/express-session": "^1.18.0",
    "@types/supertest": "^6.0.2",
    "aws-sdk": "^2.1691.0",
    "body-parser-xml": "^2.0.5",
    "bullmq": "^5.12.12",
    "cloudevents": "^8.0.2",
    "commander": "^12.1.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.4.5",
    "errors": "^0.3.0",
    "express": "^4.19.2",
    "express-jwt": "^8.4.1",
    "express-openapi-validator": "^5.3.3",
    "express-session": "^1.18.0",
    "fs": "^0.0.1-security",
    "js-yaml": "^4.1.0",
    "jwks-rsa": "^3.1.0",
    "lint-staged": "^15.2.10",
    "pino": "^9.3.2",
    "pino-pretty": "^11.2.2",
    "snyk": "^1.1293.1",
    "sqs-consumer": "^11.0.2",
    "supertest": "^7.0.0",
    "swagger-ui-express": "^5.0.1",
    "typeconv": "^2.3.1",
    "uuid": "^10.0.0",
    "yaml": "^2.5.1",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.5.1",
    "@types/pino-multi-stream": "^5.1.6",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^10.0.0",
    "@types/yamljs": "^0.2.34",
    "copyfiles": "^2.4.1",
    "eslint": "^9.10.0",
    "eslint-plugin-import": "^2.30.0",
    "globals": "^15.9.0",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "prisma": "^5.19.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.4.0"
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.ts": "eslint --fix"
  }
}
