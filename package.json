{
  "name": "dsp-boilerplate",
  "version": "1.0.0",
  "scripts": {
    "start": "ts-node-dev --respawn ./src/index.ts",
    "build": "tsup && npm run copy-files",
    "copy-files": "copyfiles -u 1 \"src/swagger.yaml\" \"dist\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test-repo": "npx jest src/repos/repo.test.ts",
    "postinstall": "npx prisma migrate dev --schema ./src/prisma/schema.prisma && npx prisma generate --schema ./src/prisma/schema.prisma && npx ts-node src/repos/seed.ts && npm run generate-openapi-spec",
    "generate-openapi-spec": "npx typeconv -f ts -t oapi -o src/swagger/schemas src/types/*.ts && npx ts-node src/swagger/openapi-spec-generator.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "bullmq": "^5.12.10",
    "commander": "^12.1.0",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.4.5",
    "errors": "^0.3.0",
    "express": "^4.19.2",
    "express-openapi-validator": "^5.3.1",
    "fs": "^0.0.1-security",
    "js-yaml": "^4.1.0",
    "pino": "^9.3.2",
    "pino-http": "^10.2.0",
    "pino-multi-stream": "^6.0.0",
    "pino-pretty": "^11.2.2",
    "swagger-ui-express": "^5.0.1",
    "typeconv": "^2.3.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.5.0",
    "@types/pino-multi-stream": "^5.1.6",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/yamljs": "^0.2.34",
    "copyfiles": "^2.4.1",
    "jest": "^29.7.0",
    "prisma": "^5.18.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "ts-watch": "^1.0.8",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  }
}
